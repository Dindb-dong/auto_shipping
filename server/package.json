{
  "name": "auto-shipping-server",
  "version": "1.0.0",
  "description": "로지뷰 물류 발송 대행 시스템 백엔드",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t auto-shipping-server .",
    "docker:run": "docker run -p 3000:3000 auto-shipping-server",
    "docker:scan": "docker scout cves auto-shipping-server",
    "docker:clean": "docker system prune -f"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.9",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "pg": "^8.11.3",
    "undici": "^6.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
